{
    "version": 2,
    "functions": {
        "api/*.py": {
            "runtime": "@vercel/python"
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "api/index.py"
        },
        {
            "source": "/(.*)",
            "destination": "dist_root/$1"
        },
        {
            "source": "/",
            "destination": "dist_root/index.html"
        }
    ]
}